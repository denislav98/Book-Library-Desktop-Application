<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane id="AnchorPane" fx:id="rootPane" prefHeight="299.0" prefWidth="541.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="library.addNewBook.AddNewBookController">
   <children>
      <VBox layoutX="20.0" layoutY="18.0" prefHeight="336.0" prefWidth="491.0" stylesheets="@../../css/style/someCss.css">
         <children>
            <JFXTextField fx:id="title" promptText="Book Title">
               <VBox.margin>
                  <Insets left="10.0" right="10.0" top="20.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="id" layoutX="20.0" layoutY="30.0" promptText="Book ID">
               <VBox.margin>
                  <Insets left="10.0" right="10.0" top="20.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="author" layoutX="10.0" layoutY="55.0" promptText="Autor">
               <VBox.margin>
                  <Insets left="10.0" right="10.0" top="20.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="publisher" layoutX="10.0" layoutY="80.0" promptText="Publisher">
               <VBox.margin>
                  <Insets left="10.0" right="10.0" top="20.0" />
               </VBox.margin>
            </JFXTextField>
            <HBox prefHeight="68.0" prefWidth="491.0">
               <children>
                  <JFXButton fx:id="saveButton" onAction="#addBook" prefHeight="104.0" prefWidth="250.0" text="Save">
                     <HBox.margin>
                        <Insets left="10.0" right="5.0" />
                     </HBox.margin>
                  </JFXButton>
                  <JFXButton fx:id="cancelButton" layoutX="10.0" layoutY="10.0" onAction="#cancelBook" prefHeight="122.0" prefWidth="250.0" text="Cancel">
                     <HBox.margin>
                        <Insets left="5.0" right="5.0" />
                     </HBox.margin>
                  </JFXButton>
               </children>
               <VBox.margin>
                  <Insets bottom="5.0" top="10.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
